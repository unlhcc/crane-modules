#%Module1#####################################################################

proc ModulesHelp {} {
    global help_message
    puts stderr "\t$help_message\n"
}



set install_root /util/opt
set package_name abaqus
set package_version 6.14.2


set help_message "Loads $package_name $package_version."
module-whatis $help_message

unsetenv SLURM_GTIDS


set package_root $install_root/$package_name
prepend-path PATH $package_root/Commands
